<Window x:Class="proyectofinal.nuevaVentana"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="nuevaVentana" Height="300" Width="300">
    <Grid x:Name="Proveedores">
        <Label x:Name="TutuloProv" Content="Datos Proveedor" HorizontalAlignment="Left" Margin="99,10,0,0" VerticalAlignment="Top" Width="101"/>
        <Label x:Name="nombre" Content="Nombre" HorizontalAlignment="Left" Height="29" Margin="10,63,0,0" VerticalAlignment="Top" Width="70"/>
        <TextBox x:Name="txtnom" HorizontalAlignment="Left" Height="23" Margin="99,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="direccion" Content="Direccion" HorizontalAlignment="Left" Height="29" Margin="10,110,0,0" VerticalAlignment="Top" Width="70"/>
        <TextBox x:Name="txtdir" HorizontalAlignment="Left" Height="23" Margin="99,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="txtgir" Content="Giro" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Width="61"/>
        <TextBox x:Name="txtgiro" HorizontalAlignment="Left" Height="23" Margin="99,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="enviarbd" Content="Enviar" HorizontalAlignment="Left" Margin="207,217,0,0" VerticalAlignment="Top" Width="75" Click="enviarbd_Click"/>

    </Grid>
</Window>
